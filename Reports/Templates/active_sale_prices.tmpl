<Html {RightToLeft}>
  <Head>
    <meta http-equiv="Content-Type" content="text/html; charset={CharacterSet}" >

    <style type="text/css">
      <!--
      body {
        background-color: #FFFFFF;
        margin: 0;
        padding: 0;
        font-style:normal; font-variant:normal; font-weight:normal; font-size:80%; font-family:verdana, arial, sans-serif; background-position-y:center
      }
      .size11 {
        font-size: 11px;
      }
      .minitype {font-size: 9px}
      .greendis {
        font-size: 11px;
        color: 00ff00;
      }
      .redneg {
        font-size: 11px;
        color: #FF0000;
      }
      .blueinc {
        font-size: 11px;
        color: 0000FF;
      }
      .blackreg {
        font-size: 11px;
        color: #000000;
      }

      -->
    </style>

    <title>{Literal}AccuPOS Active Sale Prices Report{/Literal}</title>
</Head>

<Body>
<div align="center"><br>
  <span class="size11"><strong> {Literal}Active Sale Prices Report{/Literal}</strong></span><br>
  <br>
</div>
<div align="center">
  <table width="630" border="0" align="center" cellpadding="0" cellspacing="0">
    <tr>
      <td><div align="center"><span class="size11"><strong>{CompanyName}</strong></span><span class="size11"><br>
{CompanyAddress1} {CompanyAddress2}<br>
{CompanyCity}, {CompanyState}&nbsp;{CompanyZip}</span></div></td>
    </tr>
    <tr>
      <td>&nbsp;</td>
    </tr>
    <tr>
      <td><div align="center"><span class="size11">        {Literal}Report was run on{/Literal}</span><span class="size11"> {DateRun}</span></div></td>
    </tr>
  </table>
</div>
<hr align="center" width="630" size="1">
<div align="center">
  <table width="724" border="0" align="center" cellpadding="0" cellspacing="0">
    <tr>
      <td width="216"><img src="images/spacer.aci" width="126" height="8"></td>
      <td width="108"><div align="left">
        <img src="images/spacer.aci" width="102" height="8"></div></td>
      <td width="168"><img src="images/spacer.aci" width="100" height="8"></td>
      <td width="104"><img src="images/spacer.aci" width="102" height="8"></td>
      <td width="128"><div align="left"><img src="images/spacer.aci" width="126" height="8"></div></td>
    </tr>
    <tr>
      <td valign="top" class="size11" width="216"><div align="left"><strong>{Literal}Description:{/Literal}</strong></div></td>
      <td valign="top" class="size11" width="108"><div align="left">&nbsp;</div></td>
      <td valign="top" class="size11" width="168"><div align="left"><strong>{Literal}Start:{/Literal}</strong></div></td>
      <td valign="top" class="size11" width="104"><strong>{Literal}End:{/Literal}</strong></td>
      <td valign="top" class="size11" width="128"><div align="left"><strong>{Literal}Discount:{/Literal}</strong></div></td>
    </tr>
  {PriceBlock}
    <tr>
      <td width="216"><img src="images/spacer.aci" width="126" height="1"></td>
      <td width="108"><div align="left">
        <img src="images/spacer.aci" width="106" height="5"></div></td>
      <td width="168"><img src="images/spacer.aci" width="100" height="1"></td>
      <td width="104"><img src="images/spacer.aci" width="102" height="1"></td>
      <td width="128"><div align="left"><img src="images/spacer.aci" width="126" height="1"></div></td>
    </tr>
  <tr>
    <td valign="top" class="size11" width="216"><div align="left">{Description}</div></td>
    <td valign="top" class="size11" width="108"><div align="left">{Name}</div></td>
    <td valign="top" class="size11" width="168"><div align="left">{Start}</div></td>
    <td valign="top" class="size11" width="104">{End}</td>
    <td valign="top" class="size11" width="128"><div align="left">{Discount}</div></td>
  </tr>
{/PriceBlock} 
  </table>
</div>
<hr align="center" width="630" size="1" noshade>
<div align="center"><span class="minitype">&nbsp;&nbsp;Powered by AccuPOS Point Of Sale&nbsp;rev02</span></div>
</Body>
</Html>{DateFormat}MM-dd-yy HH:mm{/DateFormat}
{PercentFormat}###0.00%;-###0.00%{/PercentFormat}
{PriceFormat}$###0.00;-$###0.00{/PriceFormat}
{QuantityFormat}##0.##;-##0.##{/QuantityFormat}
{OnItemDescription}Item Discount{/OnItemDescription}
{OnTypeDescription}Group Discount{/OnTypeDescription}

{Errors}
